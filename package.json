{
    "name": "FSDBackend",
    "version": "1.0.0",
    "description": "Express JS application",
    "main": "app.js",
    "type": "module",
    "scripts": {
        "setup": "npm install && python3 -m pip install -r requirements.txt",
        "start": "node app.js",
        "dev": "nodemon app.js",
        "test:subject": "node --experimental-vm-modules node_modules/jest/bin/jest.js Subject.test.js --detectOpenHandles",
        "test:auth": "node --experimental-vm-modules node_modules/jest/bin/jest.js Auth.test.js --detectOpenHandles",
        "test:notification": "node --experimental-vm-modules node_modules/jest/bin/jest.js Notification.test.js --detectOpenHandles"
    },
    "dependencies": {
        "axios": "^1.8.4",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-session": "^1.18.1",
        "form-data": "^4.0.2",
        "google-auth-library": "^9.15.1",
        "he": "^1.2.0",
        "jest": "^29.7.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.10.1",
        "multer": "^1.4.5-lts.1",
        "node-cron": "^4.0.7",
        "node-fetch": "^2.7.0",
        "nodemailer": "^7.0.3",
        "nodemon": "^3.1.9",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "supertest": "^7.0.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "supertest": "^7.0.0"
    }
}
